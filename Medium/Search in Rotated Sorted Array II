class Solution {
    public boolean search(int[] nums, int target) {
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==target)
            {
            return true;
            }

        }
        return false;

    }
}
//Example 1:

Input: nums = [2,5,6,0,0,1,2], target = 0
Output: true
