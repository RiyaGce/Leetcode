class Solution {
    public int[] plusOne(int[] digits) {
        
       int len=digits.length-1;
       for(int i=len;i>=0;i--)
       {
            if(digits[i]<9)
            {
            ++digits[i];
            return digits;
            }
            digits[i]=0;
       }
            int arr[]=new int[digits.length+1];
             arr[0]=1;
            return arr;

        
    }
}
